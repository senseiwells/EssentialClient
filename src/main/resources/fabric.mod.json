{
  "schemaVersion": 1,
  "id": "essential-client",
  "version": "${version}",
  "name": "Essential Client",
  "description": "A client mod that provides utilities and client support for carpet rules",
  "authors": [
    "Sensei"
  ],
  "contributors": [
    "AngelBottomless",
    "BvngeeCord",
    "Crec0",
    "Earthcomputer",
    "HardCoded",
    "lhmddws",
    "Pixeils",
    "Stove",
    "SuperSanta"
  ],
  "contact": {
    "source": "https://github.com/senseiwells/EssentialClient"
  },
  "license": "MIT",
  "icon": "assets/essential-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.senseiwells.essential_client.EssentialClient"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "me.senseiwells.essential_client.compat.ModMenuEssentialClientImpl"
      }
    ]
  },
  "mixins": [
    "essential-client.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "${minecraft_dependency}"
  }
}
